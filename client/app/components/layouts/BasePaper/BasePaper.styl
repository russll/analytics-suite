.base-paper__appbar-store
  @media screen and (max-width: 840px)
    display none

.base__content-wrapper_dashboard
  width 100%
  padding-top 24px
  padding-bottom 96px
  display inline-block
  vertical-align top

.base__content_dashboard
  overflow-y scroll
  position absolute
  top 0
  left 96px
  padding-top 40px
  padding-left 40px
  width 100%
  height 100%
  z-index 10

.base-paper
  padding 0
  @media screen and (max-width: 840px)
    width 100%
    padding-top 64px
  @media screen and (min-width: 840px)
    width auto
    margin-bottom 64px
    position absolute
    top 64px
    right 104px
    left 104px
    z-index 5

.base-paper_left
  @media screen and (min-width: 840px)
    left 420px